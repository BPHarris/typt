# Presentation example showing loop scopes

def int_list_to_string(my_list: List[Int]) -> String:
    return_value : String = '['

    for i in my_list:
        return_value += int_to_string(i) + ', '

    return_value += ']'

    # Uncomment to show error
    # i = 10

    return return_value


# Print list to show program execution
print(int_list_to_string(range(0, 5)))
