# Presentation example showing loop scopes

def int_list_to_string(my_list: List[Int]) -> String:
    return_value : String = '['

    for i in my_list:
        return_value += int_to_string(i) + ', '

    return_value += ']'

    # Uncomment to show error
    # print_int(i)

    return return_value


